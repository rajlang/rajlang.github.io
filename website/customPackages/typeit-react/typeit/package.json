    },
    "devDependencies": {
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-typescript": "^7.15.0",
        "jest": "^27.3.1",
        "jest-cli": "^27.3.1",
        "typescript": "^4.4.4",
        "vite": "^2.6.10"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "<rootDir>/__tests__/setup.js"
